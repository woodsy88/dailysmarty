@font-face {

    font-family: 'MuseoSans-300Italic';

    src: url(<%= '../fonts/MuseoSans-300Italic.eot' %>);

    src: url(<%= '../fonts/MuseoSans-300Italic.eot' %>) format('embedded-opentype'),

        url(<%= '../fonts/MuseoSans-300Italic.eot' %>) format('woff'),

        url(<%= '../fonts/MuseoSans-300Italic.eot' %>) format('truetype'),

        url(<%= '../fonts/MuseoSans-300Italic.eot' %>) format('svg');

}

